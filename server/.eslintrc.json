{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "quotes": [
            "warn",
            "single"
        ],
        "indent": [
            "warn",
            2
        ],
        "semi": [
            "warn",
            "never"
        ],
        "no-console": "warn",
        "no-undef": "warn"
    }
}
